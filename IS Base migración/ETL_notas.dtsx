                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select distinct
p.idprofesor,
case when cedula is null then pasaporte when pasaporte is null then cedula else 'NO REGISTRA' end as identificacion, 
isnull(trim(apellidos),'SD') as apellidos, 
isnull(trim(nombre),'SD') as nombre,
case when especializacion='' then 'NO REGISTRA' else ISNULL(especializacion, 'NO REGISTRA') end as especializacion, 
isnull(substring(upper(sexo), 1,1),'SD') as sexo,
case when instruccion='' OR instruccion=NULL then 'NO REGISTRA' else ISNULL(instruccion, 'NO REGISTRA') end as instruccion, 
case when ca.descripcion='' OR ca.descripcion=NULL then 'NO REGISTRA' else ISNULL(ca.descripcion, 'NO REGISTRA') end as categoria, 
case when co.descripcion='' then 'NO REGISTRA' else ISNULL(upper(co.descripcion), 'NO REGISTRA') end as colectivo, 
case when com.descripcion ='' then 'NO REGISTRA' else ISNULL(com.descripcion, 'NO REGISTRA') end as comision,
case when nf.descripcion='' then 'NO REGISTRA' ELSE ISNULL(nf.descripcion,'NO REGISTRA') end as nivel_formacion,
case when p.iddedicacion=1 then 'MEDIO TIEMPO' when p.iddedicacion=2 then 'TIEMPO PARCIAL' when p.iddedicacion=3 then 'TIEMPO COMPLETO' else 'NO REGISTRA' end as dedicacion,
ISNUll(ec.descripcion,'NO REGISTRA') as estado_civil,
case 
	when p.directivo=1 and p.codirectivo=0 then 'DIRECTIVO' 
	when p.directivo=0 and p.codirectivo=1 then 'CODIRECTIVO'
	when p.directivo=1 and p.codirectivo=1 then 'DIRECTIVO Y CODIRECTIVO'
	else 'NINGUNO'
