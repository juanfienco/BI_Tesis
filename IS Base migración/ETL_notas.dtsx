                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
p.idpensumenfasismateriatipo as id_materias_pensum,
f.idestructura as idfacultad,
c.idcarrera,
e.idenfasis,
p.idpensum,
m.idmateria,
max(f.denominacion) as facultad, 
max(c.descripcion) as carrera,
max(e.descripcion) as enfasis,
max(pe.descripcion) as pensum,
max(m.codmateria) as codmateria, 
max(m.descripcion) as materia,
max(a.descripcion) as eje,
string_agg(convert(nvarchar, pr.idmateriapre),',') as prerequisito_ids,
string_agg(convert(nvarchar, m2.descripcion), ',') as prerequisito_desc,
string_agg(convert(nvarchar, m2.codmateria), ',') as prerequisito_codmateria,
string_agg(convert(nvarchar, cr.idmateriaco),',') as corequisito_ids,
string_agg(convert(nvarchar, m3.descripcion), ',') as corequisito_desc,
string_agg(convert(nvarchar, m3.codmateria), ',') as corequisito_codmateria,
isnull(max(p.creditos),0) as creditos, 
isnull(max(p.credito_real), 0) as creditos_real,
max(na.descripcion) as nivel
FROM
dat_pensum_materia_enfasis_tipomateria p
INNER JOIN dat_pensum pe ON pe.idpensum=p.idpensum
INNER JOIN dat_carrera as c on pe.idcarrera=c.idcarrera
INNER JOIN dat_facultad as f on f.idestructura = c.idfacultad
INNER JOIN dat_enfasis e ON e.idenfasis=p.idenfasis
INNER JOIN dat_materia m ON m.idmateria=p.idmateria
INNER JOIN dat_area a ON a.idarea=p.idarea
INNER JOIN nom_area_general as na ON a.idareageneral = na.idareageneral
inner join dat_area_credito as ac on a.idarea = ac.idarea and ac.idpensum=pe.idpensum
left join dat_pre_requisitos_materia as pr on p.idmateria = pr.idmateria
left join dat_materia m2 ON m2.idmateria=pr.idmateriapre
left join dat_co_requisitos_materia as cr on p.idmateria = cr.idmateria
left join dat_materia m3 ON m3.idmateria=cr.idmateriaco
/*where m.descripcion ='ELABORACIÓN DEL TRABAJO DE TITULACIÓN'*/
group by f.idestructura, c.idcarrera, p.idpensum, e.idenfasis, a.idarea, m.idmateria, m.descripcion, na.descripcion,p.idpensumenfasismateriatipo
order by p.idpensumenfasismateriatipo, f.idestructura, c.idcarrera, p.idpensum, na.descripcion, m.descripcion</property>
