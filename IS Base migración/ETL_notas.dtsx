                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select distinct
t.id as sk_tiempo,
pd.id as sk_periodo,
md.idalumno_mate_deta,
car.id as sk_carrera,
pe.id as sk_malla,
c.id as sk_cursos, 
a.id as sk_alumno,
doc.id as sk_docente,
isnull(ta.descripcion,'SD') as tipo_aprobado,
tt.descripcion as tipo_alumno,
max(md.nota) as nota,
isnull(sum(f.faltas),0) as faltas
from dat_alumno_mate_deta as md
inner join dat_periododocente as pdoc on md.idperiodo = pdoc.idperiododocente
inner join DW_sga.dbo.Dim_periodo as pd on pdoc.idperiodogeneral = pd.idperiodogeneral
inner join DW_sga.dbo.Dim_alumno as a on a.idalumno = md.idalumno
inner join DW_sga.dbo.Dim_tiempo as t on t.Fecha = convert(date,md.fecha)
inner join dat_tipoaprobado as ta on md.idtipoaprobado = ta.idtipoaprobado
left join dat_tipo_alumno as tt on md.idtipoalumno_his = tt.idtipoalumno
left join DW_sga.dbo.Dim_cursos as c on md.idcurso=c.idcurso
left join DW_sga.dbo.Dim_carrera as car on car.idcarrera = md.idcarrera_period
left join dat_curso_dat_profesor_dat_aula_dat_horariodetallado as hcpd on hcpd.idcurso = md.idcurso
left join dat_profesor as prof on prof.idprofesor = hcpd.idprofesor
left join DW_sga.dbo.Dim_docente as doc on doc.idprofesor = prof.idprofesor
left join DW_sga.dbo.Dim_pensum as pe on pe.idpensum = c.idpensum and pe.idcarrera = c.idcarrera and pe.idmateria=c.idmateria and pe.idfacultad=c.idfacultad
left join dat_falta as f on f.idperiododocente = md.idperiodo and f.idmateria=md.idmateria and f.idalumno = md.idalumno
group by md.idalumno_mate_deta, pd.id, car.id, c.id, pe.id, a.id, doc.id, t.id, ta.descripcion, tt.descripcion</property>
