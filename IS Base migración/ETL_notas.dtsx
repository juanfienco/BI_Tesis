                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
a.idalumno,
isnull(trim(upper(a.apellidos)),'NO REGISTRA') as apellidos, 
isnull(trim(upper(a.nombre)),'NO REGISTRA') as nombre, 
isnull(trim((concat(trim(upper(a.apellidos)), ' ',trim(upper(a.nombre))))), 'NO REGISTRA') as estudiante,
isnull(a.sexo,'-') as sexo, 
isnull(trim(upper(a.nacionalidad)),'NO REGISTRA') as nacionalidad, 
case when len(a.lugar_nacimiento)=0 then 'NO REGISTRA' else isnull(trim(upper(a.lugar_nacimiento)),'NO REGISTRA') end as lugar_nacimiento,
isnull(trim(ta.descripcion),'NO REGISTRA') as tipo_alumno, 
isnull(trim(upper(c.descripcion)),'NO REGISTRA') as canton_domicilio, 
isnull(trim(upper(c1.descripcion)),'NO REGISTRA') as canton_nacimiento,
isnull(trim(upper(p.descripcion)),'NO REGISTRA') as provincia_domicilio, 
isnull(trim(upper(p1.descripcion)),'NO REGISTRA') as provincia_nacimiento
FROM dat_alumno as a
inner join dat_tipo_alumno as ta on a.idtipoalumno=ta.idtipoalumno
left join dat_canton as c on a.idcanton = c.idcanton
left join dat_canton as c1 on a.id_canton_nac = c1.idcanton
left join dat_provincia as p on p.idprovincia = c.idprovincia
left join dat_provincia as p1 on a.idprovincia_resid = p1.idprovincia</property>
