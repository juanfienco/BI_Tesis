                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
max(d1.idcurso) as idcurso,
max(d1.idperiododocente) as idperiodo,
max(d3.idestructura) as idfacultad,
max(d5.idcarrera) as idcarrera,
(select max(c2.idpensum) from dat_pensum c2 where c2.idcarrera = d5.idcarrera ) as idpensum,
max(d4.idmateria) as idmateria,
isnull((select string_agg(s3.idprofesor,', ') within group (order by s3.apellidos, s3.nombre) from dat_curso s1
	inner join dat_curso_dat_profesor s2 on s1.idcurso= s2.idcurso
	inner join dat_profesor s3 on s2.idprofesor = s3.idprofesor
	where s1.idcurso = d1.idcurso),0) as iddocentes,
max(d3.denominacion) AS facultad, 
max(d5.descripcion) as carrera,
(select max(c2.descripcion) from dat_pensum c2 where c2.idcarrera = d5.idcarrera ) as pensum,
isnull((select distinct string_agg(concat(s5.descripcion , '--' , s6.descripcion) ,', ') from dat_pensum_materia_enfasis_tipomateria s2  --distinct 
	inner join dat_area s4 on s2.idarea=s4.idarea
	inner join nom_area_general s5 on s4.idareageneral = s5.idareageneral
	inner join dat_enfasis s6 on s2.idenfasis = s6.idenfasis
	where s2.idpensum = (select max(c2.idpensum) from dat_pensum c2 where c2.idcarrera = d5.idcarrera
	and  c2.idcarrera = d5.idcarrera) and s2.idmateria = d4.idmateria and s6.idenfasis in (select distinct(g1.idenfasis) from dat_enfasis g1 
    where g1.idcarrera = d5.idcarrera)),'SD') as nivel_itinerario,
max(d4.descripcion) as asignatura,
isnull(max(char(64 + d1.par_curs)),'-') as paralelo,
max(case when d1.plan_contingencia=1 then 'Si' else 'No' End) as plan_contingencia,
max(d1.cupo) as cupo,
(select count(distinct(c1.idalumno)) from dat_alumno_mate_deta c1
	where c1.idcurso=d1.idcurso) as total_estudiantes_registrados,
isnull((select string_agg(concat(s3.apellidos ,' ',s3.nombre),', ') within group (order by s3.apellidos, s3.nombre) from dat_curso s1
	inner join dat_curso_dat_profesor s2 on s1.idcurso= s2.idcurso
	inner join dat_profesor s3 on s2.idprofesor = s3.idprofesor
	where s1.idcurso = d1.idcurso),'SD') as docente,
isnull((select sum(DATEDIFF(MINUTE,f4.hora_inicio,f4.hora_fin)) from dat_curso f1 
inner join dat_curso_dat_profesor f2 on f1.idcurso = f2.idcurso
inner join dat_horario f3 on f1.idhorario=f3.idhorario
inner join dat_horario_detallado f4 on f3.idhorario=f4.idhorario
inner join dat_curso_dat_aula f5 on f2.idcurso=f5.idcurso
inner join dat_curso_dat_profesor_dat_aula_dat_horariodetallado f6 on f5.idcurso=f6.idcurso and f2.idprofesor = f6.idprofesor and f4.idhorario_detallado=f6.idhorariodetallado and f5.idaula=f6.idaula	where f1.idcurso = d1.idcurso),0) as horas,
max(d1.estado) as estado_curso, 
isnull(max(d7.detalle),'SD') as horario,
isnull(max(d2.descripcion),'SD') as periodo_academico
from dat_curso d1
inner join dat_periododocente d2 on d1.idperiododocente=d2.idperiododocente
inner join dat_facultad d3 on d1.idfacultad=d3.idestructura
inner join dat_materia d4 on d1.idmateria = d4.idmateria
inner join dat_carrera d5 on d1.idcarrera = d5.idcarrera
inner join dat_horario d7 on d1.idhorario = d7.idhorario
group by d3.idestructura,d5.idcarrera, d1.idcurso, d1.par_curs,d7.idhorario,d4.idmateria, d2.descripcion
order by facultad, carrera, nivel_itinerario, asignatura,paralelo</property>
