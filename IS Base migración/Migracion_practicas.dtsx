                <property
                  dataType="System.String"
                  description="Especifica la instrucción SQL que genera la tabla de búsqueda."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select idcarrera, codsenescyt, descripcion, 
(case 
when ISNUMERIC(codsenescyt )=1 
	then convert(nvarchar, TRY_CONVERT(numeric,codsenescyt ))
	else 
		(case when CHARINDEX('-',codsenescyt)&gt;0 then
			substring(
			replace(replace(replace(codsenescyt ,'-P-','-'),'1016-5-',''),'1016-3-',''),0,
			charindex('-',replace(replace(replace(codsenescyt ,'-P-','-'),'1016-5-',''),'1016-3-',''))
		)
		else
			codsenescyt
		end)
 end) as codigo
from dat_carrera 
where estado = 1 and codsenescyt is not null and not codsenescyt in ('0')
and not idcarrera in (1001338, 1001441,1001442,1001443,1001445)
order by 4</property>
