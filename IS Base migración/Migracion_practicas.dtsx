                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select distinct p.idcarrera, p.descripcion_carrera,
ISNUMERIC(p.codigo_carrera) as es_numerico,
(case 
when ISNUMERIC(p.codigo_carrera)=1 
	then convert(nvarchar, TRY_CONVERT(numeric, p.codigo_carrera))
	else 
		substring(
		replace(replace(replace(p.codigo_carrera ,'-P-','-'),'1016-5-',''),'1016-3-',''),0,
		charindex('-',replace(replace(replace(p.codigo_carrera ,'-P-','-'),'1016-5-',''),'1016-3-','')) 
		)
end) as codigo_carrera
from dat_practicas_preprofesionales as p 
where codigo_carrera is not null and descripcion_carrera is not null
--and codigo_carrera = '522' --650721A-P-01'
--and codigo_carrera like'%-%'
order by 4</property>
